<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Clash of Virtual Machines</title>
	<link rel="stylesheet" href="<%= static_path(@conn, "/css/app.css") %>">
	<script src="js/modernizr.custom.js"></script>
</head>
<body>
	<div id="page">
		<div id="header">
			<div class="background">
				<h1 id="logo"> <a href="index.html">Clash of Virtual Machines</a> </h1>
				<div id="navigation">
					<ul>
						<li class="selected">
							<a href="vote">Votación</a>
						</li>
						<li class="selected">
							<a href="results">Resultados</a>
						</li>
					</ul>
				</div>
			</div>
		</div>
		<div id="contents">
			<div id="adbox">
				<div id=centeredVote >
					<dl>
						<dt>    Virtual Machines  </dt>
							<%= for entry <- @entries do %>
							  <dd class="percentage percentage-<%= entry.percentage %>"><span class="text"><%= entry.name %>: <%= entry.percentage %>%</span></dd>
							<% end %>
					</dl>
				</div>
			</div>
		</div>
		<div id="footer">
			<div class="background">
				<div class="body">
					<p id="footnote">
						© Copyright 2023. All rights reserved.
					</p>
				</div>
			</div>
		</div>
	</div>
</body>
</html>